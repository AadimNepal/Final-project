{
    "env": {
      "node": true,
      "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
      "indent": ["error", 2],
      "linebreak-style": ["error", "unix"],
      "quotes": ["error", "single"],
      "semi": ["error", "always"],
      "no-console": "off",
      "import/extensions": ["error", "always", {
        "js": "never",
        "mjs": "always"
      }]
    }
  }